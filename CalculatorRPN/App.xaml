<Application x:Class="CalculatorRPN.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:CalculatorRPN"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="btnBackgroundNumberColor" Color="White"/>
        <SolidColorBrush x:Key="btnForegroundNumberColor" Color="#585555"/>

        <SolidColorBrush x:Key="btnBackgroundOperationColor" Color="#4c5866"/>
        <SolidColorBrush x:Key="btnForegroundOperationColor" Color="White"/>
    </Application.Resources>
</Application>
